<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="318.0" prefWidth="359.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="it.unipi.gamegram.LoginController">
   <children>
      <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="35.0" prefWidth="88.0" text="Nick" textAlignment="CENTER">
         <font>
            <Font size="18.0" />
         </font>
         <VBox.margin>
            <Insets left="15.0" right="15.0" top="5.0" />
         </VBox.margin>
      </Label>
      <TextField fx:id="signInNick" prefHeight="0.0" prefWidth="159.0">
         <VBox.margin>
            <Insets bottom="15.0" left="100.0" right="100.0" top="5.0" />
         </VBox.margin>
      </TextField>
      <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="35.0" prefWidth="104.0" text="Password" textAlignment="CENTER">
         <font>
            <Font size="18.0" />
         </font>
         <VBox.margin>
            <Insets left="15.0" right="15.0" top="5.0" />
         </VBox.margin>
      </Label>
      <PasswordField fx:id="signInPassword" prefHeight="0.0" prefWidth="159.0">
         <VBox.margin>
            <Insets bottom="15.0" left="100.0" right="100.0" top="5.0" />
         </VBox.margin>
      </PasswordField>
      <Button fx:id="login" mnemonicParsing="false" onAction="#login" prefHeight="40.0" prefWidth="88.0" text="Login">
         <font>
            <Font size="18.0" />
         </font>
         <VBox.margin>
            <Insets left="120.0" right="120.0" top="5.0" />
         </VBox.margin>
      </Button>
      <Button fx:id="back" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#back" prefHeight="40.0" prefWidth="88.0" text="Back" textAlignment="CENTER">
         <font>
            <Font size="18.0" />
         </font>
         <VBox.margin>
            <Insets left="120.0" right="120.0" top="5.0" />
         </VBox.margin>
      </Button>
      <Label fx:id="errorMessage" alignment="CENTER" prefHeight="46.0" prefWidth="270.0">
         <font>
            <Font size="18.0" />
         </font>
         <VBox.margin>
            <Insets bottom="5.0" left="15.0" right="15.0" top="5.0" />
         </VBox.margin>
      </Label>
   </children>
</VBox>
