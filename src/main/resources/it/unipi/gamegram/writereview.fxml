<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="476.0" prefWidth="506.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="it.unipi.gamegram.WriteReviewController">
   <children>
      <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="35.0" prefWidth="88.0" text="Title" textAlignment="CENTER">
         <font>
            <Font size="24.0" />
         </font>
         <VBox.margin>
            <Insets left="15.0" right="15.0" top="2.0" />
         </VBox.margin>
      </Label>
      <TextField fx:id="reviewTitle" prefHeight="26.0" prefWidth="270.0">
         <font>
            <Font size="18.0" />
         </font>
         <VBox.margin>
            <Insets left="100.0" right="100.0" top="2.0" />
         </VBox.margin>
      </TextField>
      <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="35.0" prefWidth="110.0" text="Text" textAlignment="CENTER">
         <font>
            <Font size="24.0" />
         </font>
         <VBox.margin>
            <Insets left="15.0" right="15.0" top="2.0" />
         </VBox.margin>
      </Label>
      <TextArea fx:id="reviewText" prefHeight="200.0" prefWidth="513.0">
         <VBox.margin>
            <Insets left="100.0" right="100.0" top="2.0" />
         </VBox.margin>
         <font>
            <Font size="18.0" />
         </font>
      </TextArea>
      <Button fx:id="submit" mnemonicParsing="false" onAction="#submit" prefHeight="46.0" prefWidth="122.0" text="Submit">
         <font>
            <Font size="18.0" />
         </font>
         <VBox.margin>
            <Insets left="15.0" right="15.0" top="5.0" />
         </VBox.margin>
      </Button>
      <Button fx:id="back" mnemonicParsing="false" onAction="#back" prefHeight="46.0" prefWidth="122.0" text="Back">
         <font>
            <Font size="18.0" />
         </font>
         <VBox.margin>
            <Insets left="15.0" right="15.0" top="5.0" />
         </VBox.margin>
      </Button>
      <Label fx:id="outcomeMessage" alignment="CENTER" prefHeight="46.0" prefWidth="514.0">
         <VBox.margin>
            <Insets bottom="2.0" left="15.0" right="15.0" top="5.0" />
         </VBox.margin>
         <font>
            <Font size="18.0" />
         </font>
      </Label>
   </children>
</VBox>
