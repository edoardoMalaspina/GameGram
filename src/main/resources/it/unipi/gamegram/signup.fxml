<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="390.0" prefWidth="382.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="it.unipi.gamegram.SignupController">
   <children>
      <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="35.0" prefWidth="88.0" text="Name" textAlignment="CENTER">
         <font>
            <Font size="18.0" />
         </font>
         <VBox.margin>
            <Insets left="15.0" right="15.0" top="2.0" />
         </VBox.margin>
      </Label>
      <TextField fx:id="signUpName" prefHeight="26.0" prefWidth="270.0">
         <VBox.margin>
            <Insets left="100.0" right="100.0" />
         </VBox.margin>
      </TextField>
      <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="35.0" prefWidth="110.0" text="Surname" textAlignment="CENTER">
         <font>
            <Font size="18.0" />
         </font>
         <VBox.margin>
            <Insets left="15.0" right="15.0" top="2.0" />
         </VBox.margin>
      </Label>
      <TextField fx:id="signUpSurname" prefHeight="26.0" prefWidth="270.0">
         <VBox.margin>
            <Insets left="100.0" right="100.0" />
         </VBox.margin>
      </TextField>
      <Label fx:id="nick" alignment="CENTER" contentDisplay="CENTER" prefHeight="35.0" prefWidth="110.0" text="Nick">
         <font>
            <Font size="18.0" />
         </font>
         <VBox.margin>
            <Insets left="15.0" right="15.0" top="2.0" />
         </VBox.margin>
      </Label>
      <TextField fx:id="signUpNick" prefHeight="26.0" prefWidth="270.0">
         <VBox.margin>
            <Insets left="100.0" right="100.0" />
         </VBox.margin>
      </TextField>
      <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="35.0" prefWidth="110.0" text="Password">
         <font>
            <Font size="18.0" />
         </font>
         <VBox.margin>
            <Insets left="15.0" right="15.0" top="2.0" />
         </VBox.margin>
      </Label>
      <PasswordField fx:id="signUpPassword" prefHeight="26.0" prefWidth="270.0">
         <VBox.margin>
            <Insets left="100.0" right="100.0" />
         </VBox.margin>
      </PasswordField>
      <Button fx:id="signup" alignment="CENTER" mnemonicParsing="false" onAction="#signup" prefHeight="46.0" prefWidth="122.0" text="Signup">
         <font>
            <Font size="18.0" />
         </font>
         <VBox.margin>
            <Insets top="5.0" />
         </VBox.margin>
      </Button>
      <Button fx:id="back" alignment="CENTER" mnemonicParsing="false" onAction="#back" prefHeight="46.0" prefWidth="122.0" text="Back">
         <font>
            <Font size="18.0" />
         </font>
         <VBox.margin>
            <Insets top="5.0" />
         </VBox.margin>
      </Button>
      <Label fx:id="outcomeMessage" alignment="CENTER" prefHeight="24.0" prefWidth="270.0">
         <font>
            <Font size="18.0" />
         </font>
         <padding>
            <Insets bottom="2.0" top="2.0" />
         </padding>
         <VBox.margin>
            <Insets bottom="2.0" top="2.0" />
         </VBox.margin>
      </Label>
   </children>
</VBox>
