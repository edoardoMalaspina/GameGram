<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="466.0" prefWidth="616.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="it.unipi.gamegram.ReviewPageController">
   <children>
      <Label fx:id="title" alignment="CENTER" prefHeight="96.0" prefWidth="586.0" textAlignment="CENTER" wrapText="true">
         <font>
            <Font size="36.0" />
         </font>
         <VBox.margin>
            <Insets bottom="5.0" left="15.0" right="15.0" top="5.0" />
         </VBox.margin>
      </Label>
      <Label fx:id="game" prefHeight="38.0" prefWidth="584.0">
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
      </Label>
      <Label fx:id="author" prefHeight="38.0" prefWidth="584.0">
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
      </Label>
      <Label fx:id="date" prefHeight="98.0" prefWidth="584.0">
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
      </Label>
      <Label fx:id="reviewText" prefHeight="234.0" prefWidth="584.0" wrapText="true">
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
      </Label>
      <Button fx:id="delete" alignment="CENTER" mnemonicParsing="false" onAction="#delete" prefHeight="40.0" prefWidth="136.0" text="Delete Review">
         <font>
            <Font size="18.0" />
         </font>
         <VBox.margin>
            <Insets bottom="5.0" left="15.0" right="15.0" top="10.0" />
         </VBox.margin>
      </Button>
      <Button fx:id="back" contentDisplay="CENTER" mnemonicParsing="false" onAction="#back" prefHeight="40.0" prefWidth="136.0" text="Back">
         <font>
            <Font size="18.0" />
         </font>
         <VBox.margin>
            <Insets bottom="15.0" left="15.0" right="15.0" top="5.0" />
         </VBox.margin>
      </Button>
   </children>
</VBox>
