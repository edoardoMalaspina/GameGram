<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox fx:id="chartContainer" alignment="CENTER" style="-fx-background-color: FDFBE2;" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="it.unipi.gamegram.MostReviewedTrendController">
   <children>
      <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="108.0" prefWidth="580.0" text="Most reviewed game trend" textAlignment="CENTER" textFill="#ceb700">
         <font>
            <Font size="36.0" />
         </font>
         <VBox.margin>
            <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
         </VBox.margin>
      </Label>
      <AnchorPane fx:id="pane" prefHeight="299.0" prefWidth="580.0">
         <VBox.margin>
            <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
         </VBox.margin>
      </AnchorPane>
      <Button fx:id="back" mnemonicParsing="false" onAction="#back" prefHeight="26.0" prefWidth="111.0" style="-fx-background-color: CEB700;" text="Back" textFill="WHITE">
         <font>
            <Font size="18.0" />
         </font>
         <VBox.margin>
            <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
         </VBox.margin>
      </Button>
   </children>
</VBox>
