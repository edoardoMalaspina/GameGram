            <Font size="15.0" />
         </font>
         <VBox.margin>
            <Insets left="15.0" right="15.0" top="2.0" />
         </VBox.margin>
      </Label>
      <TextField fx:id="gameFullDescription" layoutX="110.0" layoutY="316.0" prefHeight="26.0" prefWidth="270.0">
         <VBox.margin>
            <Insets bottom="2.0" left="100.0" right="100.0" />
         </VBox.margin>
      </TextField>
      <Button fx:id="insert" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#insert" prefHeight="15.0" prefWidth="95.0" style="-fx-background-color: CEB700;" text="Insert" textAlignment="CENTER" textFill="WHITE">
         <VBox.margin>
            <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
         </VBox.margin>
         <font>
            <Font size="15.0" />
