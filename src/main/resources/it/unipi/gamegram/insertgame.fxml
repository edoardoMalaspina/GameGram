<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="453.0" prefWidth="350.0" style="-fx-background-color: FDFBE2;" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="it.unipi.gamegram.InsertGameController">
   <children>
      <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="35.0" prefWidth="275.0" text="Name" textAlignment="CENTER" textFill="#ceb700">
         <font>
            <Font size="15.0" />
         </font>
         <VBox.margin>
            <Insets left="15.0" right="15.0" top="2.0" />
         </VBox.margin>
      </Label>
      <TextField fx:id="gameName" prefHeight="26.0" prefWidth="179.0">
         <VBox.margin>
            <Insets left="100.0" right="100.0" />
         </VBox.margin>
      </TextField>
      <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="35.0" prefWidth="238.0" text="Date of Publication" textAlignment="CENTER" textFill="#ceb700">
